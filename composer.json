{
    "name": "webit/bcmath",
    "description": "Object wrapper for PHP BcMath Library",
    "type": "library",
    "license": "MIT",
    "keywords": ["bcmath", "big numeric"],
    "authors": [{
    	"name": "Daniel Bojdo",
        "email": "daniel.bojdo@web-it.eu"
    }],
    "autoload": {
        "psr-4": { "Webit\\Wrapper\\BcMath\\": "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Webit\\Wrapper\\BcMath\\": "tests/" }
    },
    "require": {
        "php": ">=7.1",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.20"
    },
    "extra": {
        "branch-alias": {
            "dev-1.x": "1.x-dev",
            "dev-master": "2.x-dev"
        }
    }
